input:
  bg: "oklch(0.96 0.02 240)"
  text: "oklch(0.25 0.03 240)"
  border: "oklch(0.85 0.02 240)"
  placeholder: "oklch(0.75 0.02 240)"

keybind:
  bg: "oklch(0.85 0.02 210)"
  text: "oklch(0.25 0.03 240)"
  border: "oklch(0.78 0.02 210)"

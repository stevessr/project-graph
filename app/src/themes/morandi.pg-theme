  NodeDetailsText: "#414a54" # 深冷灰文字
  CollideBoxPreSelected: "rgba(169, 221, 208, 0.3)" # 浅豆绿预选
  CollideBoxSelected: "rgba(62, 125, 83, 0.4)" # 冷绿选中
  CollideBoxPreDelete: "rgba(255, 255, 0, 0.2)" # 补充默认预删状态

effects:
  flash: "#8b949d" # 冷淡蓝闪烁
  dash: "#6b7280" # 薰衣草拖影
  windowFlash: "#eef1f0" # 背景同色窗口闪烁
  warningShadow: "#b1b7bd" # 冷灰蓝警告阴影
  successShadow: "#7893af" # 冷蓝成功阴影
